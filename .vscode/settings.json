{
    "clang-tidy.executable": "clang-tidy -I include",
    "editor.formatOnSave": true,
    "c-cpp-flylint.flexelint.enable": false,
    "c-cpp-flylint.language": "c",
    "c-cpp-flylint.ignoreParseErrors": true,
    "c-cpp-flylint.flawfinder.enable": false,
    "c-cpp-flylint.cppcheck.extraArgs": [
        " --check-level=exhaustive"
    ],
    "files.associations": {
        "array": "c",
        "deque": "c",
        "string": "c",
        "unordered_map": "c",
        "vector": "c",
        "string_view": "c",
        "format": "c",
        "initializer_list": "c",
        "span": "c",
        "text_encoding": "c",
        "stdbool.h": "c",
        "dict.h": "c",
        "random": "c",
        "algorithm": "c",
        "functional": "c"
    }
}